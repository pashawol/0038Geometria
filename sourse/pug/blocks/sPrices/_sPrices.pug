mixin sPrices(data)
	// start sPrices
	+b.SECTION.sPrices.section#sPrices&attributes(attributes)
		.container
			+b.section-title.text-center
				h2 Цены на окна
			.tabs.tabs--js
				.tabs__caption 
					.tabs__btn.active VEKA Softline
					.tabs__btn VEKA WHS72
				.tabs__wrap
					mixin tab()
						.tabs__content&attributes(attributes)
							.row
								mixin item(t,i)
									+e.col.col-xl-4
										+e.item
											+e.head
												+e.title.d-none.d-xl-block!= t
												+pic(`prod-${i}.png`,'','') 
											+e.body
												+e.title.d-block.d-xl-none!= t
												+in-c("radio",'',`radio-${i}`)(checked)
													strong 14 900 руб
													span / окно
														+e.tooltip ?
															+e.tooltip-text Lorem ipsum dolor sit, amet consectetur adipisicing elit. Nemo, ipsum.
													
												+in-c("radio",'',`radio-${i}`)
													strong 31 900 руб
													span / под ключ
														+e.tooltip ?
															+e.tooltip-text Lorem ipsum dolor sit, amet consectetur adipisicing elit. Nemo, ipsum. 
												+e.btn.btn-outline-primary(href="#modal-call").link-modal-js Уточнить цену
								+item(`Двухстворчатое окно с одной створкой глухой, другой поворотной VEKA Softline`,1)
								+item('Трехстворчатое окно с центральной поворотной створкой VEKA Softline',2)
								+item('Балконный блок с поворотной дверью VEKA Softline',3)
					+tab.active
					+tab
					.text-center
						+e.btn.btn-primary(href="#sCalc")
							+icon('calculator')
							| Рассчитать цену в калькуляторе
						
	// end sPrices